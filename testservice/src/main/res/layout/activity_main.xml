<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="deepin.testservice.MainActivity">

    <Button
        android:id="@+id/id_start_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启动intent" />

    <Button
        android:id="@+id/id_stop_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭intent" />

    <Button
        android:id="@+id/id_bind_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bind" />

    <Button
        android:id="@+id/id_unbind_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="unbind" />

    <Button
        android:id="@+id/id_set_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="设置数据" />

    <Button
        android:id="@+id/id_get_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取数据" />
</LinearLayout>
